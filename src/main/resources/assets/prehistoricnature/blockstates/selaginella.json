{
  "multipart": [
  	
    { "when": { "OR": [
    	{ "facing": "north" },
		{ "south": true }
    	]},
      "apply": [
      		{ "model": "prehistoricnature:block/selaginella", "x": 90 },
      		{ "model": "prehistoricnature:block/selaginella_1", "x": 90 },
      		{ "model": "prehistoricnature:block/selaginella_2", "x": 90 }
      		]
    },
    
    { "when": { "OR": [
    	{ "facing": "east" },
		{ "west": true }
    	]},
      "apply": [
	      	{ "model": "prehistoricnature:block/selaginella", "x": 90, "y": 90 },
	      	{ "model": "prehistoricnature:block/selaginella_1", "x": 90, "y": 90 },
	      	{ "model": "prehistoricnature:block/selaginella_2", "x": 90, "y": 90 }
	      	]
    },
    
    { "when": { "OR": [
    	{ "facing": "south" },
		{ "north": true }
    	]},
      "apply": [
      		{ "model": "prehistoricnature:block/selaginella", "x": 90, "y": 180 },
      		{ "model": "prehistoricnature:block/selaginella_1", "x": 90, "y": 180 },
      		{ "model": "prehistoricnature:block/selaginella_2", "x": 90, "y": 180 }
      		]
    },

    { "when": { "OR": [
    	{ "facing": "west" },
		{ "east": true }
    	]},
      "apply": [
      		{ "model": "prehistoricnature:block/selaginella", "x": 90, "y": 270 },
      		{ "model": "prehistoricnature:block/selaginella_1", "x": 90, "y": 270 },
      		{ "model": "prehistoricnature:block/selaginella_2", "x": 90, "y": 270 }
      		]
    },

    { "when": { "OR": [
    	{ "facing": "up" },
		{ "down": true }
    	]},
      "apply": [
      		{ "model": "prehistoricnature:block/selaginella" },
      		{ "model": "prehistoricnature:block/selaginella_1" },
      		{ "model": "prehistoricnature:block/selaginella_2" }
      		]
    },
    
    { "when": { "OR": [
    	{ "facing": "down" },
		{ "up": true }
    	]},
      "apply": [
      		{ "model": "prehistoricnature:block/selaginella", "x": 180 },
      		{ "model": "prehistoricnature:block/selaginella_1", "x": 180 },
      		{ "model": "prehistoricnature:block/selaginella_2", "x": 180 }
      		]
    }
    
  ]
}