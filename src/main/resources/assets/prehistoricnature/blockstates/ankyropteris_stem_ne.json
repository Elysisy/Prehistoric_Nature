            "apply": [
			      {"model": "prehistoricnature:block/ankyropteris_stem_corner", "x": 180 }
			    ]
        },
        {   "when": { "facing": "north" },
            "apply": [
			      {"model": "prehistoricnature:block/ankyropteris_leaves", "y": 180 }
			    ]
